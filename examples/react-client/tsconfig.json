{
	"compilerOptions": {
		"target": "ES2022",
		"lib": ["ES2022", "DOM", "DOM.Iterable"],
		"module": "ESNext",
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"allowImportingTsExtensions": true,
		"strict": true,
		"noEmit": true,
		"jsx": "react-jsx",
		"skipLibCheck": true
	},
	"include": ["src"]
}
